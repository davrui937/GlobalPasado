package Pruebas;

import java.util.Scanner;

public class aceptaelreto148 {
	private static Scanner teclado;
	static final int TAM2=2;
	
public static void main(String[] args) {
	    teclado = new Scanner(System.in);
	    String hora;
	    int min=0;
	   
	    System.out.println("Inserta la hora:");
				hora=teclado.nextLine();
				
			//hora= hora.replace(":"," ");
				
			char[] array = hora.toCharArray();			
			
			System.out.println(array);
			 
	        int entero = Character.getNumericValue(array[0]);
	        entero=entero*10;
	        entero=entero+Character.getNumericValue(array[1]);
	        
	        System.out.println(entero);

	    
	}
}